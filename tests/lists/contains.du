/**
 * contains.du
 *
 * Testing the list.contains() method
 *
 * .contains() returns a boolean if the given value exists within the list
 */

var x = [1, 2, 3, 4, 5, [6, 7]];

// First check the list was created properly
assert(x == [1, 2, 3, 4, 5, [6, 7]]);

// Test .contains()
assert(x.contains(1));
assert(x.contains(2));
assert(x.contains(3));
assert(x.contains(4));
assert(x.contains(5));
assert(x.contains([6, 7]));

// Test not  .contains()
assert(not x.contains(6));
assert(not x.contains(7));
assert(not x.contains(10));
assert(not x.contains(11));
assert(not x.contains(12));
assert(not x.contains(13));
assert(not x.contains(14));
