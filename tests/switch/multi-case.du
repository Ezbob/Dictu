/**
 * multi-case.du
 *
 * Testing multicase within a switch
 */

var x = 10;

switch (x) {
    case 1, 2, 3, 4: {
        x += 10;
    }

    case 5, 6, 7, 8, 9: {
        x += 10;
    }

    case 10: {
        x += 10;
    }
}

assert(x == 20);

// Default + reorder

var x = 10;

switch (x) {
    case 1, 2, 3, 4: {
        x += 10;
    }

    case 10: {
        x += 10;
    }

    case 5, 6, 7, 8, 9: {
        x += 10;
    }

    default: {
        x += 100;
    }
}

assert(x == 20);