/**
 * data-types.du
 *
 * Testing data-types within a switch
 */

var x = 0;

switch ("string") {
    case 1: {
        x += 10;
    }

    case "string": {
        x += 10;
    }

    case [1, 2]: {
        x += 10;
    }
}

assert(x == 10);

switch ([1, 2]) {
    case 1: {
        x += 10;
    }

    case "string": {
        x += 10;
    }

    case [1, 2]: {
        x += 10;
    }
}

def func() {}

assert(x == 20);

switch (func) {
    case 1: {
        x += 10;
    }

    case "string": {
        x += 10;
    }

    case [1, 2]: {
        x += 10;
    }

    case func: {
        x += 10;
    }
}

assert(x == 30);